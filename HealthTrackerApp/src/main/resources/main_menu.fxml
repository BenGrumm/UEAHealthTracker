<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.registerController">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="248.0" GridPane.columnIndex="1">
               <items>
                  <Button disable="true" mnemonicParsing="false" text="Diet &amp; Exercise" />
                  <Button disable="true" mnemonicParsing="false" text="Goals" />
                  <Button disable="true" mnemonicParsing="false" text="Groups" />
               </items>
            </ToolBar>
            <ToolBar prefHeight="40.0" prefWidth="352.0">
              <items>
                <Button mnemonicParsing="false" onAction="#changeScene" text="Register" />
                  <Button mnemonicParsing="false" text="Log in" />
              </items>
            </ToolBar>
         </children>
      </GridPane>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Firstname:" />
            <TextField />
            <Label text="Surname:" />
            <TextField />
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="100.0" prefHeight="40.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Username:" />
            <TextField />
            <Label layoutX="128.0" layoutY="22.0" text="Email:" />
            <TextField />
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="140.0" prefHeight="47.0" prefWidth="597.0" spacing="10.0">
         <children>
            <Label text="Password:" />
            <PasswordField />
            <Label text="Password Verification:" />
            <PasswordField layoutX="130.0" layoutY="21.0" />
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" prefHeight="75.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Weight (Stone):" />
            <Spinner fx:id="weightStoneSpinner" editable="true" onMouseClicked="#spinnerClick">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </Spinner>
            <Label text="Weight (Pounds):" />
            <Spinner fx:id="weightPoundsSpinner" editable="true" onMouseClicked="#spinnerClick">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></Spinner>
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="167.0" prefHeight="75.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Gender:" />
            <ChoiceBox fx:id="genderBox" prefWidth="150.0" />
            <Label layoutX="253.0" layoutY="39.0" text="Height (cm):" />
            <Spinner fx:id="heightSpinner" editable="true" onMouseClicked="#spinnerClick">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </Spinner>
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
      <Button mnemonicParsing="false" onAction="#changeScene" text="Register" />
   </children>
</VBox>
