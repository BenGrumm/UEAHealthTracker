<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="hBox" alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER" prefWidth="220.0">
        <children>
            <ImageView>
                <image>
                    <Image url="@icons/baseline_menu_book_black_24dp.png" />
                </image>
            </ImageView>
            <Text fx:id="typeText" text="Food Type" />
        </children>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" prefWidth="100.0">
        <children>
            <ImageView>
                <image>
                    <Image url="@icons/baseline_food_bank_black_24dp.png" />
                </image>
            </ImageView>
            <Text fx:id="mealText" text="Meal Consumed" />
        </children>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" prefWidth="100.0">
        <children>
            <ImageView>
                <image>
                    <Image url="@icons/baseline_local_dining_black_24dp.png" />
                </image>
            </ImageView>
            <Text fx:id="calsText" text="Calories Consumed" />
        </children>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <HBox alignment="CENTER" prefWidth="100.0">
        <children>
            <ImageView>
                <image>
                    <Image url="@icons/baseline_today_black_24dp.png" />
                </image>
            </ImageView>
            <Text fx:id="dateText" text="Date" />
        </children>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </HBox>
    <Button fx:id="deleteCellButton" mnemonicParsing="false">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
        <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </HBox.margin>
        <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@icons/baseline_delete_black_24dp.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
</HBox>
